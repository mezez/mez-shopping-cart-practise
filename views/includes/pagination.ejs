<section class="pagination">
        <%  if (currentPage !== 1 && previousPage !== 1){ %><a href="?page=1">1</a><% } %><!--used != instead of !== because a string is being compared to a number-->
        <% if (hasPreviousPage){ %><a href="?page=<%=previousPage%>"><%=previousPage%></a><% } %>
        <a href="?page=<%=currentPage%>" class="active"><%=currentPage%></a>
        <% if (hasNextPage){ %><a href="?page=<%=nextPage%>"><%=nextPage%></a><% } %>
        <% if ((lastPage !== currentPage) && (nextPage !== lastPage)){ %><a href="?page=<%=lasttPage%>"><%=lastPage%></a><% } %>
    </section>